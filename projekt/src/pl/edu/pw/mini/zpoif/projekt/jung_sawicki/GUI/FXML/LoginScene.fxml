<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600" prefWidth="800.0"
          xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="pl.edu.pw.mini.zpoif.projekt.jung_sawicki.GUI.LoginController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="232.0" minWidth="10.0" prefWidth="160.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="358.0" minWidth="10.0" prefWidth="235.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="383.0" minWidth="10.0" prefWidth="211.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="305.0" minWidth="10.0" prefWidth="182.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submit" text="Zatwierdź" GridPane.columnIndex="2"
            GridPane.halignment="CENTER" GridPane.rowIndex="3">
        <GridPane.margin>
            <Insets/>
        </GridPane.margin>
    </Button>
    <Label text="Wpisz nazwę użytkownika" GridPane.columnIndex="1" GridPane.halignment="CENTER"
           GridPane.rowIndex="2"/>
    <TextField fx:id="userName" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
    <Button fx:id="newUserButton" onAction="#newUser" mnemonicParsing="false" text="Dodaj nowego użytkownika"
            GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3"/>
</GridPane>
